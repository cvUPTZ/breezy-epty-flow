
{
  "version": 2,
  "functions": {
    "python-detection-service/sota_main.py": {
      "runtime": "python3.9",
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "python-detection-service/sota_main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "python-detection-service"
  }
}
